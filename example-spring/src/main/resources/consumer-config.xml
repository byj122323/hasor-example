<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd" default-autowire="byName">

    <import resource="classpath:common-config.xml"/>

    <!-- EchoService -->
    <bean id="echoService" class="net.hasor.rsf.spring.RsfConsumerBean" init-method="init">
        <property name="bindType" value="net.demo.client.consumer.EchoService"/>
        <!--<property name="target" value="rsf://127.0.0.1:2180/default"/>-->
    </bean>
    <!-- MessageService -->
    <bean id="messageService" class="net.hasor.rsf.spring.RsfConsumerBean" init-method="init">
        <property name="bindType" value="net.demo.client.consumer.MessageService"/>
        <!--<property name="target" value="rsf://127.0.0.1:2180/default"/>-->
    </bean>
    <!-- UserService -->
    <bean id="userService" class="net.hasor.rsf.spring.RsfConsumerBean" init-method="init">
        <property name="bindType" value="net.demo.client.consumer.UserService"/>
        <!--<property name="target" value="rsf://127.0.0.1:2180/default"/>-->
    </bean>

</beans>